{
  "$schema": "https://unpkg.com/wrangler@latest/config-schema.json",
  "name": "mcp-url-fetcher",
  "main": "worker.ts",
  "compatibility_date": "2025-08-14",
  "observability": { "logs": { "enabled": true } },
  "durable_objects": {
    "bindings": [{ "name": "HISTORY_DO", "class_name": "HistoryDO" }]
  },
  "migrations": [{ "tag": "v1", "new_sqlite_classes": ["HistoryDO"] }],
  "route": { "custom_domain": true, "pattern": "mcp.llmtext.com" }
}
